# http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = "utf8"
end_of_line = "LF"


[*.css]
indent_style = tab
indent_size = 2
trim_trailing_whitespace = true


[*.js]
indent_style = tab
indent_size = 2
trim_trailing_whitespace = true
quote_type = double


[*.json]
indent_style = tab
indent_size = 2
trim_trailing_whitespace = true
